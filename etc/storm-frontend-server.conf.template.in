##### StoRM frontend configuration file #####
fe.port = 8444
fe.threadpool.threads.number = 50
#fe.gsoap.maxpending = 1000                      # default is 1000

# Log options
log.filename = @localstatedir@/log/@PACKAGE_PREFIX_NAME@/storm-frontend-server.log
log.debuglevel = INFO      # Allowed values: ERROR, WARN, INFO, DEBUG, DEBUG2.

# Security settings
security.enable.vomscheck = true      # default is "true"

# Backend information
be.xmlrpc.host = localhost
be.xmlrpc.token = token
#be.xmlrpc.port = 8080    # default is 8080
#be.xmlrpc.path = /RPC2   # default is /RPC2

be.recalltable.port=9998  # default is 9998

# Proxy information
proxy.dir = @localstatedir@/tmp/@PACKAGE_PREFIX_NAME@/proxy
#proxy.user = storm       # default is storm

# Database
db.host = localhost
db.user = storm
db.passwd = password

# Argus Blacklisting
#check.user.blacklisting = false # default is false
#argus-pepd-endpoint =

#monitoring.enabled = true  # default is true
#monitoring.timeInterval = 60  # default is 60 (seconds)
#monitoring.detailed = false  # default is false


# WSDL to be returned to a GET request
wsdl.file = @datadir@/wsdl/srm.v2.2.wsdl

