##### StoRM frontend configuration file #####
fe.threads.number = 50
fe.port = 8444

# Log options
log.filename = @prefix@/var/log/storm-frontend.log
log.debuglevel = DEBUG2

# Security settings
security.disable.mapping = false
security.disable.vomscheck = false
#security.gridmapfile = /etc/grid-security/grid-mapfile
#security.hostcert = /etc/grid-security/hostcert.pem
#security.hostkey = /etc/grid-security/hostkey.pem

# Backend XMLRPC server information
be.xmlrpc.host = devrb.cnaf.infn.it
#be.xmlrpc.host = devwn01.cnaf.infn.it
#be.xmlrpc.port = 8080
#be.xmlrpc.path = /RPC2

proxy.dir = @prefix@/var/proxy
#proxy.user = 

# Database
db.host = localhost
db.user = storm
db.passwd = password

# WSDL to be returned to a GET request
wsdl.file = @prefix@/wsdl/srm.v2.2.wsdl

